{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-alert": 0,
    "no-console": 2,
    "no-unused-vars": 1,
    "camelcase": 2,
    "no-var": 2,
    "no-empty": 2,
    "no-unreachable": 0,
    "eqeqeq": 2,
    "max-lines": ["error", 3],
    "semi": ["error", "always"],
    "quotes": ["error", "single"]
  }
}
// rules
//  "key": "value => off/0, warning/1, error/2"
